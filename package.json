{
  "name": "azure-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/validateData.js && node scripts/fetchEntraIdRolesIfPossible.js && npm run generate-ip-lookup-index && next build && node scripts/generateSitemap.js",
    "analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "eslint .",
    "update-ip-data": "ts-node -r tsconfig-paths/register scripts/updateIpData.ts",
    "update-rbac-data": "ts-node -r tsconfig-paths/register scripts/updateRbacData.ts",
    "fetch-entraid-roles": "ts-node --project tsconfig.node.json scripts/fetchEntraIdRoles.ts",
    "generate-actions-cache": "ts-node -r tsconfig-paths/register scripts/generateActionsCache.ts",
    "generate-ip-indexes": "ts-node -r tsconfig-paths/register scripts/generateIpIndexes.ts",
    "generate-ip-lookup-index": "ts-node -r tsconfig-paths/register scripts/generateIpLookupIndex.ts"
  },
  "dependencies": {
    "@azure/identity": "^4.5.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.2.0",
    "gray-matter": "^4.0.3",
    "ip-cidr": "^4.0.2",
    "js-base64": "^3.7.8",
    "next": "^16.1.6",
    "papaparse": "^5.5.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "rehype-prism-plus": "^2.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.6",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^25.2.3",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-unused-imports": "^4.4.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsconfig-paths": "^4.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
