# Azure AD App Registration (required for tenant lookup API)
AZURE_TENANT_ID=
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=

# Optional: CORS origins (comma-separated)
# TENANT_LOOKUP_ALLOWED_ORIGINS=

# Optional: Rate limiting (tenant lookup API)
# RATE_LIMIT_REQUESTS=10
# RATE_LIMIT_WINDOW_MS=60000

# Optional: Override Graph API endpoints
# AZURE_AUTHORITY_HOST=
# GRAPH_SCOPE=
# GRAPH_BASE_URL=

# Optional: Client-side API base URL
# NEXT_PUBLIC_TENANT_LOOKUP_API_BASE=

# Optional: Development flags
# DEBUG_UPDATE_IP_DATA=false
# ANALYZE=false
